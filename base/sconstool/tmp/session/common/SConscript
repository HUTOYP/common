#-mode python-
Import('env')
env = env.Clone()

libsname=['']
libcommon_sources = Glob('*.c[cp]*')

env.aLibrary(target = 'session_common',
             source = libcommon_sources,
	     LIBS = libsname
             )	  

commontest = env.aDirs('test')

env.aSDKIncludes(Glob('*.h'))

cpps=[libcommon_sources]
Return(['cpps'] )

